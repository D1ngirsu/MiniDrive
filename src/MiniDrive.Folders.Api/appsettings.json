{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "FoldersDb": "Server=(localdb)\\MSSQLLocalDB;Database=MiniDrive_Folders;Trusted_Connection=True;TrustServerCertificate=True"
  },
  "Redis": {
    "ConnectionString": "localhost:6379,abortConnect=false,connectTimeout=5000",
    "KeyPrefix": "minidrive:folders:",
    "DefaultTtl": "00:30:00"
  },
  "Services": {
    "Identity": "http://localhost:5001"
  },
  "Observability": {
    "Jaeger": {
      "Enabled": false,
      "Host": "localhost",
      "Port": 4317
    }
  }
}
